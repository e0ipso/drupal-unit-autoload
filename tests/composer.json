{
  "require": {
    "phpunit/phpunit": "4.7.*",
    "mockery/mockery": "0.9.*",
    "satooshi/php-coveralls": "0.6.*"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\Composer\\ClassLoader\\": "../src/",
      "Drupal\\Composer\\ClassLoader\\Tests\\": "src/",
      "Drupal\\MyModule\\": "DRUPAL_CONTRIB<my_module>/src/",
      "": "DRUPAL_ROOT/includes"
    },
    "psr-0": {
      "": "DRUPAL_ROOT/includes"
    },
    "drupal-path": {
      "\\Tmp": "DRUPAL_ROOT/file.inc",
      "\\Tmp2": "data/acme.inc"
    }
  }
}
